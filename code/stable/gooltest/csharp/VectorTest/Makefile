ifeq "$(OS)" "Windows_NT"
    TARGET_EXTENSION=.exe
    CSC=csc
else
    UNAME_S := $(shell uname -s)
    ifeq ($(UNAME_S), Linux)
        TARGET_EXTENSION=
        CSC=mcs
    endif
    ifeq ($(UNAME_S), Darwin)
        TARGET_EXTENSION=
        CSC=mcs
    endif
endif

# Generated by Drasil v0.1-alpha

# Project Name: VectorTest

build: VectorTest$(TARGET_EXTENSION)

VectorTest$(TARGET_EXTENSION): VectorTest.cs
	$(CSC) -out:VectorTest$(TARGET_EXTENSION) VectorTest.cs

run: build
	./VectorTest$(TARGET_EXTENSION) $(RUNARGS)

.PHONY: build run
