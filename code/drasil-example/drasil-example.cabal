Name:		drasil-example
Version:	0.1.23
Cabal-Version:  >= 1.18
Author:		Dan Szymczak, Steven Palmer, Jacques Carette, Spencer Smith
build-type:     Simple
Synopsis:	A framework for code and document generation for scientific software - Example SubPackage

executable tiny
  hs-source-dirs: .
  main-is: Drasil/HGHC/Main.hs
  other-modules:
      Drasil.HGHC.HGHC
    , Drasil.HGHC.HeatTransfer
  Build-Depends:
    base >= 4.7,
    lens >= 4.12,
    containers >= 0.5.5.1,
    pretty >= 1.1.1.1,
    mtl >= 2.2.1,
    directory >= 1.2.6.2,
    split >= 0.2.3.1,
    drasil-lang >= 0.1.58,
    drasil-data >= 0.1.11,
    drasil-code >= 0.1.7,
    drasil-database >= 0.1.0,
    drasil-printers >= 0.1.8,
    drasil-gen >= 0.1.3,
    drasil-docLang >= 0.1.24,
    drasil-theory >= 0.1.0,
    drasil-utils >= 0.1.0
  default-language: Haskell2010
  ghc-options:      -Wall -O2 -Wredundant-constraints

