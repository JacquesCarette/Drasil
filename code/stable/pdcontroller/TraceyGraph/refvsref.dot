	qdPropControlFD -> qdProcessErrorFD;
	qdPropControlFD -> laplaceRC:theory;
	qdDerivativeControlFD -> qdProcessErrorFD;
	qdDerivativeControlFD -> laplaceRC:theory;
	qdCtrlVarFD -> qdPropControlFD;
	qdCtrlVarFD -> qdDerivativeControlFD;
	gdPowerPlantRC:theory -> laplaceRC:theory;
	gdPowerPlantRC:theory -> tmSOSystemRC:theory;
