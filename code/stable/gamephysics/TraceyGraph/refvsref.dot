digraph refvsref {
	gravitationalAccelMK -> forceMK;
	accjMK -> p_CM;
	accjMK -> linearDisplacement;
	accjMK -> linearVelocity;
	accjMK -> linearAcceleration;
	accjMK -> newtonSLMK;
	accjMK -> gravitationalAccelMK;
